{
    "name": "apo-photoshop",
    "main": "app/index.html",
    "version": "3.0.0",
    "private": true,

    "scripts": {
        "start": "nodewebkit",
        "count": "find app/scripts/core -name '*.js' | xargs wc -l | sort -r"
    },

    "window": {
        "title": "APO: Photoshop",
        "width": 1024,
        "height": 600,
        "toolbar": false,
        "frame": true
    },

    "devDependencies": {
        "gulp": "latest",
        "gulp-jshint": "latest",
        "jshint-stylish": "latest",
        "gulp-help": "latest",
        "gulp-rename": "latest",
        "gulp-markdown-pdf": "latest",
        "cjson": "latest",
        "nodewebkit": "latest",
        "node-webkit-builder": "latest",
        "del": "latest"
    }
}
